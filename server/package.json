{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prod": "cp .env_prod .env && tsc -p tsconfig_prod.json && node ./dist/index.js",
    "dev": "cp .env_dev .env && nodemon ./src/index.ts",
    "start": "cp .env_dev .env && tsc && node ./dist/src/index.ts",
    "test": "TEST=true NODE_ENV=test npx jest --coverage",
    "testAuth": "cp .env_test .env && jest --detectOpenHandles --forceExit auth.test.ts",
    "testPosts": "cp .env_test .env && jest --detectOpenHandles --forceExit posts.test.ts",
    "testFile": "cp .env_test .env && jest --detectOpenHandles --forceExit file.test.ts"
  },
  "keywords": [],
